num1  = "00111"
num2 = "001010"
c = len(num1)
d = len(num2)
result = ""
a=0
b=0 
while a<c and b<d:
    temp = int(num1[a]) | int(num2[b])
    a+=1
    b+=1
    str_temp = str(temp)
    result = result + str_temp
while(a<c):
    result = result + str(num1[a])
    a+=1
while(b<d):
    result = result + str(num2[b])
    b+=1

print(result)
